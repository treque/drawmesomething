<Window x:Class="PolyPaint.Views.ChatWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:converters="clr-namespace:FMUD.Converters"
        xmlns:local="clr-namespace:PolyPaint.Views"
        mc:Ignorable="d"
        Title="Chat" Height="1000" Width="400" ResizeMode="NoResize" WindowStartupLocation="CenterScreen"
        >
    <DockPanel x:Name="mainPanel" Background="{StaticResource MaterialDesignLightBackground}">
        <local:ChatUC x:Name="ChatUC" Margin="0 0 2.5 0"></local:ChatUC>
    </DockPanel>
</Window>
